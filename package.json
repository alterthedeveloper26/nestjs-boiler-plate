{
  "name": "rbac-boiler-plate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist && rimraf tsconfig.build.tsbuildinfo",
    "build": "nest build -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "yarn build && node --max_old_space_size=2560 dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeORM": "---------------------------ORM-&-DB-----------------------------",
    "typeorm:cli": "dotenv -e .env -- typeorm-ts-node-commonjs",
    "migration:generate": "yarn build && cross-var npm run typeorm:cli -- -d dist/config/typeorm-cli-migration.config.js migration:generate -o -p ./src/migrations/$npm_config_name",
    "migration:create": "cross-var npm run typeorm:cli -- migration:create -o ./src/migrations/$npm_config_name",
    "migration:run": "yarn typeorm:cli -- -d dist/config/typeorm-cli-migration.config.js migration:run",
    "migration:revert": "yarn typeorm:cli -- -d dist/config/typeorm-cli-migration.config.js migration:revert",
    "migration:drop": "yarn typeorm:cli -- -d dist/config/typeorm-cli-migration.config.js schema:drop",
    "prepare": "husky install"
  },
  "dependencies": {
    "@faker-js/faker": "^7.5.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^10.3.3",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^10.3.3",
    "accesscontrol": "^2.2.1",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^7.3.0",
    "helmet": "^7.1.0",
    "nest-winston": "^1.7.0",
    "nestjs-cls": "^4.2.0",
    "newrelic": "^9.1.0",
    "nocache": "^4.0.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "socket.io": "^4.7.4",
    "tiny-types": "^1.21.0",
    "typeorm": "^0.3.10",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^9.0.1",
    "winston": "^3.8.2",
    "winston-syslog": "^2.6.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/lodash": "^4.14.202",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^9.0.11",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
